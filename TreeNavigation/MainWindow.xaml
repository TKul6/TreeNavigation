<Window x:Class="TreeNavigation.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="MainWindow" Height="350" Width="525">
    <StackPanel>
        <WrapPanel>
        <Label Content="Navigate to: "></Label>
            <TextBlock x:Name="SearchTBox" Text="{Binding NavigationPhrase}"></TextBlock>
        </WrapPanel>
        <TextBlock x:Name="Temp"></TextBlock>
        </StackPanel>
</Window>
